    "dexie-react-hooks": "^4.2.0",
    "firebase": "^10.13.2",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^2.5.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.2.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.5",
    "zod": "^4.1.12"
